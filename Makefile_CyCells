CC            = gcc
CXX           = g++
DEFINES       = -DQT_NO_DEBUG -DQT_OPENGL_LIB -DQT_GUI_LIB -DQT_CORE_LIB -DQT_SHARED
CXXFLAGS      = -pipe -O2 -Wall -W -D_REENTRANT $(DEFINES)
INCPATH       = -I/usr/share/qt4/mkspecs/linux-g++ -I. -I/usr/include/qt4/QtCore -I/usr/include/qt4/QtGui -I/usr/include/qt4/QtOpenGL -I/usr/include/qt4 -I. -I/usr/X11R6/include -I. -I.

SOURCES = action.cpp \
		cells.cpp \
		cellType.cpp \
		fileDef.cpp \
		fileInit.cpp \
		history.cpp \
		main.cpp \
		molecule.cpp \
		random.cpp \
		sense.cpp \
		tallyActions.cpp \
		tissue.cpp 
OBJECTS = action.o \
		cells.o \
		cellType.o \
		fileDef.o \
		fileInit.o \
		history.o \
		molecule.o \
		random.o \
		sense.o \
		tallyActions.o \
		tissue.o 

CyCells: CyCells.o $(OBJECTS)
	g++ CyCells.o $(OBJECTS) -o CyCells

CyCells.o: CyCells.cpp\
    tissue.h \
		cells.h \
		cell.h \
		simPoint.h \
		array3D.h \
		cellType.h \
		molecule.h \
		history.h \
		fileDef.h \
		fileInit.h
	$(CXX) -c CyCells.cpp 

action.o: action.cpp action.h \
		cells.h \
		cell.h \
		simPoint.h \
		array3D.h \
		molecule.h \
		random.h \
		tallyActions.h \
		rate.h
	$(CXX) -c $(CXXFLAGS) -o action.o action.cpp

